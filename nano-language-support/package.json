{
    "name": "nanoscript-support",
    "displayName": "NanoScript Language Support",
    "description": "Syntax highlighting and language support for .nano files",
    "version": "0.0.1",
    "publisher": "nanoscript",
    "engines": { "vscode": "^1.75.0" },
    "categories": ["Programming Languages"],
    "main": "./extension.js",
    "contributes": {
        "breakpoints": [
            { "language": "nanoscript" }
        ],
        "languages": [
            {
                "id": "nanoscript",
                "aliases": ["NanoScript", "nano"],
                "extensions": [".nano"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nanoscript",
                "scopeName": "source.nano",
                "path": "./syntaxes/nanoscript.tmLanguage.json"
            }
        ]
    }
}
